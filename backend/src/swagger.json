              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "message": {
                      "type": "string",
                      "example": "Internal Server Error"
                    }
                  }
                }
              }
            }
          }
        },
        "tags": [
          "Friends"
        ],
        "security": [
          {
            "BearerAuth": []
          }
        ],
        "parameters": [
          {
            "name": "requestId",
            "in": "path",
            "required": true,
            "deprecated": false,
            "schema": {
              "type": "string"
            },
            "example": "69819fc993ae3496ddb0641c"
          }
        ]
      }
    },
    "/friends/requests/{requestId}/decline": {
